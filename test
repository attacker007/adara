wget https://golang.org/dl/go1.15.6.linux-amd64.tar.gz && sudo tar -C /usr/local -xzf go1.15.6.linux-amd64.tar.gz && export PATH=$PATH:/usr/local/go/bin && go get -u github.com/tomnomnom/anew && go get -u github.com/tomnomnom/httprobe && git clone https://github.com/shmilylty/OneForAll && cd OneForAll && chmod +x *.py && python3 -m pip install -U pip setuptools wheel && pip3 install -r requirements.txt && clear && python3 oneforall.py --target test.com run && cd results && clear && cd temp && rm -r *.json && cat *.txt > asf && rm *.txt && mv asf asf.txt && mv asf.txt ../../.. && cd ../../.. && mv asf.txt ~/go/bin && cd ~/go/bin && wget https://github.com/projectdiscovery/subfinder/releases/download/v2.4.6/subfinder_2.4.6_linux_amd64.tar.gz && tar -xvf subfinder_2.4.6_linux_amd64.tar.gz && git clone https://github.com/projectdiscovery/nuclei-templates && cd nuclei-templates && wget https://github.com/projectdiscovery/nuclei/releases/download/v2.3.2/nuclei_2.3.2_linux_amd64.tar.gz && tar -xvf  nuclei_2.3.2_linux_amd64.tar.gz && chmod +x nuclei && wget https://github.com/Edu4rdSHL/findomain/releases/download/2.1.4/findomain-linux && chmod +x findomain-linux && mv findomain-linux findomain && ./findomain -t test.com -q > new.txt && ../subfinder -d test.com -silent > m.txt && clear && cat m.txt new.txt ../asf.txt | ../anew | ../httprobe -c 100 > ll.txt && ./nuclei -stats -l ll.txt -t cves -t default-logins -t dns -t exposed-panels -t exposed-tokens -t exposures -t fuzzing -t helpers -t headless -t network -t misconfiguration -t takeovers -t technologies -t vulnerabilities -t iot -t workflows -t miscellaneous -c 9000 -silent > tt.txt 
